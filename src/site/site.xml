<?xml version="1.0" encoding="UTF-8"?>
<project name="Maven-Checkmarx-Plugin of horhor">

    <!-- **************** this loads the fluido skin - using bootstrap **************** -->
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.2.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>

    <!-- **************** this configures the upper-left side image, alt and link **************** -->
    <bannerLeft>
        <name>Maven Checkmarx Plugin Logo</name>
        <!--<src>http://s12.postimg.org/65ui4p6sd/cx_logo_alpha.png</src>-->
        <src>http://i.imgur.com/kFD539p.png</src>
        <href>https://www.checkmarx.com/</href>
    </bannerLeft>

     <!-- **************** this configures the upper-right side image, alt and link **************** -->
    <bannerRight>
        <name>Maven Logo</name>
        <src>https://maven.apache.org/images/maven-logo-black-on-white.png</src>
        <href>https://maven.apache.org/</href>
    </bannerRight>

